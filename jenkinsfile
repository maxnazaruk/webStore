pipeline {
    agent any
    stages{
        stage('Git clone'){
            steps{
                git 'https://github.com/maxnazaruk/webStore.git'
            }
        }
        stage('Maven Test'){
            steps{
                sh 'mvn test'
            }
        }
        stage('Maven build'){
            steps{
               sh 'mvn package'
            }
        }
        
        stage('Maven deploy'){
            steps{
                echo "deploy"
            }
        }
    }
}
